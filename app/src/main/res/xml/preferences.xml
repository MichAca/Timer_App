<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.preference.PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
                                                xmlns:app="http://schemas.android.com/apk/res-auto">


    <com.pavelsikun.seekbarpreference.SeekBarPreference

            android:key = "com.example.timerapp.timer_length"
            android:title = "Timer Length"
            android:summary = "For how many minutes should the timer run?"
            android:defaultValue = "10"
            app:mbsp_minValue = "1"
            app:mbsp_maxValue = "60"
            app:mbsp_interval = "1"
            app:mbsp_measurementUnit = "minutes"
            app:mbsp_dialogEnales = "true"
    />

</android.support.v7.preference.PreferenceScreen>


